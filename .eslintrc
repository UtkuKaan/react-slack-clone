{
  "extends": ["eslint:recommended", "react-app", "prettier", "prettier/react"],
  "rules": {
    "no-console": 1
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
